{{#each this}}
  <div class='card'>
    <h1 class='card__title'>{{name}}</h1>
    <div class='card__body'>
      <div class='card__part'>
        <p><span class='card__headers'>Capital: </span>{{capital}}</p>
        <p><span class='card__headers'>Population: </span> {{population}}</p>
        <ul class='card__languages' aria-label='languages'>
          {{#each languages}}
            <li>{{this.name}}</li>
          {{/each}}
        </ul>
      </div>
      <div class='card__part'><img class='card__img' src='{{flag}}' alt='{{alpha2Code}}' /></div>
    </div>
  </div>
{{/each}}